    <script>
    // Initialize the form on page load
    document.addEventListener('DOMContentLoaded', function() {
      // Get references to form elements
      const monthRadio = document.getElementById('monthRadio');
      const yearRadio = document.getElementById('yearRadio');
      const monthSelect = document.getElementById('month');
      const yearSelect = document.getElementById('year');
      const form = document.getElementById('searchForm');
      
      // Initialize form state on page load
      if (window.location.href.includes('selected_item=month')) {
        enableMonthYear('month');
      } else if (window.location.href.includes('selected_item=year')) {
        enableMonthYear('year');
      }
      
      // Add event listeners for form validation
      form.addEventListener('submit', function(e) {
        // If no radio button is selected, show error
        if (!monthRadio.checked && !yearRadio.checked) {
          e.preventDefault();
          alert('Please select either Month or Year filter type');
          return false;
        }
        
        // If year radio is selected but no year value
        if (yearRadio.checked && !yearSelect.value) {
          e.preventDefault();
          alert('Please select a year');
          return false;
        }
        
        // If month radio is selected but missing month or year
        if (monthRadio.checked && (!monthSelect.value || !yearSelect.value)) {
          e.preventDefault();
          if (!monthSelect.value) alert('Please select a month');
          if (!yearSelect.value) alert('Please select a year');
          return false;
        }
        
        return true;
      });
    });

    function enableMonthYear(type) {
      const monthSelect = document.getElementById('month');
      const yearSelect = document.getElementById('year');
      const monthRadio = document.getElementById('monthRadio');
      const yearRadio = document.getElementById('yearRadio');
      const monthLabel = document.querySelector('label[for="monthRadio"]');
      const yearLabel = document.querySelector('label[for="yearRadio"]');
      
      // Remove active class from both
      monthLabel.classList.remove('filter-active');
      yearLabel.classList.remove('filter-active');
      
      if (type === 'month') {
        // Enable month selection
        monthSelect.disabled = false;
        yearSelect.disabled = false;
        monthSelect.required = true;
        yearSelect.required = true;
        monthRadio.checked = true;
        yearRadio.checked = false;
        // Add visual feedback
        monthLabel.classList.add('filter-active');
      } else if (type === 'year') {
        // Enable year selection only
        monthSelect.disabled = true;
        yearSelect.disabled = false;
        monthSelect.required = false;
        yearSelect.required = true;
        monthSelect.value = '';
        monthRadio.checked = false;
        yearRadio.checked = true;
        // Add visual feedback
        yearLabel.classList.add('filter-active');
      }
    }
    </script>

